      <enumeration value="onFarmGroundWater"/>
      <enumeration value="onFarmSurfaceWater">
        <annotation>
          <documentation>-- Description --
(Ponds or dams)</documentation>
        </annotation>
      </enumeration>
      <enumeration value="offFarmSurfaceWater">
        <annotation>
          <documentation>-- Description --
 from lakes, rivers or watercources.</documentation>
        </annotation>
      </enumeration>
      <enumeration value="offFarmWaterFromCommonWaterSupplyNetworks"/>
      <enumeration value="otherSources"/>
