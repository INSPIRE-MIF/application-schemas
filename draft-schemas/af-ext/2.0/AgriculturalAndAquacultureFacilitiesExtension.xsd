                </extension>
              </simpleContent>
            </complexType>
          </element>
          <element maxOccurs="unbounded" name="contains" type="af-ext:SitePropertyType"/>
          <element maxOccurs="unbounded" minOccurs="0" name="proces" type="af-ext:ProductionUnitPropertyType"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
