				<taggedValue xmlns="http://www.interactive-instruments.de/ShapeChange/AppInfo" tag="vocabulary">http://inspire.ec.europa.eu/codeList/ResultNatureValue</taggedValue>
			</appinfo>
		</annotation>
		<union memberTypes="ef:ResultNatureValueEnumerationType ef:ResultNatureValueOtherType"/>
	</simpleType>
	<simpleType name="ResultNatureValueEnumerationType">
