	<xs:simpleType name="implementationSpecification">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="discoveryImplementationSpecification">
		<xs:restriction base="implementationSpecification">
			<xs:enumeration value="CSW 2.0.2 AP ISO 1.0.0"/>
			<xs:enumeration value="OTHER"/>
		</xs:restriction>
	</xs:simpleType>
