							<xs:documentation>Maximum recursion depth performed during the pull operation</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RecachingFrequency" type="discoveryRecachingFrequency" minOccurs="0"/>
					<xs:element name="LinkScenario" type="discoveryLinkScenario" minOccurs="0"/>
