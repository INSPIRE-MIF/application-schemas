	<simpleType name="DiseaseMeasureTypeValueType">
		<annotation>
			<documentation>-- Name --
Disease measure type value

-- Definition --
Different ways how data on diseases and related health problems in a population can be reported.</documentation>
			<appinfo>
				<taggedValue xmlns="http://www.interactive-instruments.de/ShapeChange/AppInfo" tag="extensibility">none</taggedValue>
			</appinfo>
		</annotation>
		<union memberTypes="hh:DiseaseMeasureTypeValueEnumerationType hh:DiseaseMeasureTypeValueOtherType"/>
