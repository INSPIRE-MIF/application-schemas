				</annotation>
			</element>
			<element name="dimension" nillable="true">
				<annotation>
					<documentation>Size of the body used in the calculation</documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="er:EarthResourceDimension"/>
					</sequence>
					<attribute name="nilReason" type="gml:NilReasonType"/>
				</complexType>
			</element>
			<element name="proposedExtractionMethod" nillable="true">
				<annotation>
					<documentation>The method proposed to extract the commodity</documentation>
				</annotation>
			</element>
			<element name="sourceReference" maxOccurs="unbounded">
