	<element name="Resource" type="er:ResourceType" substitutionGroup="er:OreMeasure">
		<annotation>
			<documentation>A concentration or occurrence of material of intrinsic economic interest in or on the Earth&amp;rsquo;s crust in such form, quality and quantity that there are reasonable prospects for eventual economic extraction.
Synonyms: Mineral Resource; Coal Resource (s); Diamond (Gemstone) Resource; Potentially Mineable Mineralisation</documentation>
		</annotation>
	</element>
	<complexType name="ResourceType">
		<complexContent>
			<extension base="er:OreMeasureType">
				<sequence>
					<element name="category" type="er:ResourceCategoryCodeType">
						<annotation>
							<documentation>Measured, indicated, proved, probable, inferred.</documentation>
						</annotation>
					</element>
					<element name="includesReserves" nillable="true">
						<annotation>
							<documentation>Whether estimate of resources uses reserve values (Y/N)</documentation>
						</annotation>
						<complexType>
							<simpleContent>
								<extension base="boolean">
									<attribute name="nilReason" type="gml:NilReasonType"/>
								</extension>
							</simpleContent>
						</complexType>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
