							<appinfo>
								<reversePropertyName xmlns="http://www.opengis.net/gml/3.2">pf-ext:Realize</reversePropertyName>
							</appinfo>
						</annotation>
					</element>
					<element name="groupedBy" type="pf-core:InstallationPartPropertyType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>An InstallationPart is grouped by one Installation</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
